:javascript
    $(document).ready(function() {
        // Remove the js missing error from the search field.
        document.getElementById('searchField').disabled = false;
        document.getElementById('searchField').placeholder = "Search";
    });

    // Search a specific table and hide all rows that do not match.
    function doMaterialSearch(tableID) {
        var targetTable = document.getElementById(tableID);
        var searchText = document.getElementById('searchField').value.trim().toLocaleLowerCase();
        // Find and remove all search terms that are indicated to be tags
        var tagSearchTerms = searchText.match(/~\S*/gi);
        searchText = searchText.replace(/~\S*/gi,"").trim();
        if (tagSearchTerms) {
            for (var i = 0; i < tagSearchTerms.length; i++) {
                // Remove leading ~ from all tag search terms.
                tagSearchTerms[i] = tagSearchTerms[i].slice(1);
            }
        }

        var matching_rows = 0;
        var rowData = targetTable.rows;
        // Loop through the tables rows and check if they match the query.
        for (var rowIndex = 0; rowIndex < rowData.length; rowIndex++) {
            var tableRow = rowData.item(rowIndex)
            // Not a data row, ignore.
            if (tableRow.cells.length != 3)
                continue;

            // Get the search-able terms in the table.
            // We add the text to an empty string so that we don't do indexOf against null later on in the code.
            var materialName = '' + tableRow.cells.item(1).textContent.trim().toLocaleLowerCase();
            var materialUID = '' + tableRow.cells.item(0).textContent.trim().toLocaleLowerCase();
            var materialTags = '' + tableRow.cells.item(2).textContent.trim().toLocaleLowerCase().split(", ");

            var matches = false;
            // If the search term is a number only match the materials UID.
            if ($.isNumeric(searchText)) {
                // Only return true if the search result starts at the beginning of the UID
                // This will prevent a search query of 8 from returning 18.
                if (materialUID.search(searchText) == 0) {
                    matches = true;
                }
            }
            // Otherwise match the materials name
            // If searchText is "" (nothing) returns 0 instead of -1?
            // This isn't really a problem since we want the table to show everything if there is no search term.
            // This also allows us to search just by tag without having to add another else if.
            else if (materialName.indexOf(searchText) != -1) {
                matches = true;
                // If we have tags, mark any materials that don't match the tags as non-matching.
                // This will result in us only having materials that match both the query and the tags.
                if (tagSearchTerms) {
                    for (var i = 0; i < tagSearchTerms.length; i++) {
                        if (materialTags.indexOf(tagSearchTerms[i]) == -1) {
                            matches = false;
                        }
                    }
                }
            }

            // Set the current rows display attribute appropriately.
            if (matches == false) {
                tableRow.style.display = 'none';
            } else {
                matching_rows ++;
                tableRow.style.display = 'table-row';
            }
        }

        // It is easier to re-add the 'nothing found' row every time rather than tracking it during sorting.
        // This is due to the fact that it has none of the attributes a normal row has.
        var noChildElement = document.getElementById("no_items");
        if (noChildElement) {
            targetTable.removeChild(noChildElement)
        }


        // Check to see if there are no matching rows,
        // if there are none add a row indicating this.
        if (matching_rows <= 0) {
            var row = document.createElement("tr");
            var cell = document.createElement("td");
            var cellText = document.createTextNode("No materials match the query.");

            cell.appendChild(cellText);
            row.appendChild(cell);
            row.setAttribute("id", "no_items");
            cell.setAttribute("colspan", "3");
            // Insert it as the first child.
            targetTable.insertBefore(row, targetTable.firstChild);
        }
        return true;
    }
